{
  "manifest_version": 2,

  "name": "Adblock",
  "description": "This extension blocks ads that have been known to serve malware.",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}